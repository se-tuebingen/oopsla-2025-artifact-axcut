build:
	rustc -C opt-level=3 -o factorial_accumulator ../rust/factorial_accumulator.rs
	rustc -C opt-level=3 -o fibonacci_recursive ../rust/fibonacci_recursive.rs
	rustc -C opt-level=3 -o sum_range ../rust/sum_range.rs
	rustc -C opt-level=3 -o iterate_increment ../rust/iterate_increment.rs
	rustc -C opt-level=3 -o match_options ../rust/match_options.rs
	rustc -C opt-level=3 -o lookup_tree ../rust/lookup_tree.rs
	rustc -C opt-level=3 -o erase_unused ../rust/erase_unused.rs

clean:
	rm -f factorial_accumulator
	rm -f fibonacci_recursive
	rm -f sum_range
	rm -f iterate_increment
	rm -f match_options
	rm -f lookup_tree
	rm -f erase_unused
